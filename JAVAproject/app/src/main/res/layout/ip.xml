<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FFD8D8"
    android:layout_height="match_parent"
    android:baselineAligned="false">

    <Button
        android:id="@+id/button8"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:fontFamily="casual"
        android:onClick="report"
        android:text="112"
        android:textAllCaps="false"
        android:textColor="?android:attr/textColorLink"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:fontFamily="casual"
        android:text="Invasion"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="?android:attr/textColorLink"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <WebView
        android:id="@+id/WebView01"
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/txtView"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="47dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="47dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="14dp">

    </WebView>

    <TextView
        android:id="@+id/txtView"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:text="버튼을 클릭하면 감시가 시작됩니다."
        app:layout_constraintRight_toLeftOf="@+id/button13"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="32dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="98dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="29dp"
        android:layout_marginRight="32dp" />

    <Button
        android:id="@+id/button13"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:onClick="func"
        android:text="감시 시작"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="80dp"
        app:layout_constraintRight_toRightOf="@+id/WebView01"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtView"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="126dp" />

</android.support.constraint.ConstraintLayout>
